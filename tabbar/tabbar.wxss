.tabbar_box {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  width: 100%;
  position: fixed;
  bottom: 0rpx;
  left: 0;
  z-index: 999;
  background-color: #fff;
}

.middle-wrapper {
  position: absolute;
  right: 310rpx;
  bottom: 0;
  background-color: #fff;
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
}

.tabbar_nav {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  height: 100%;
  position: relative;
  padding-top: 16rpx;
}

.tabbar_icon {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 6rpx;
}

.special-wrapper {
  position: absolute;
  top: -36rpx;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  border-top: 2rpx solid #f2f2f3;
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  padding: 6rpx;
}
.tabbar_box.iphoneX-height{
  padding-bottom: 66rpx;
}

.special-wrapper .tabbar_icon {
  width: 60rpx;
  height: 60rpx;
  margin-top: 14rpx;
}

.special-text-wrapper {
  width: 46rpx;
  height: 42rpx;
}
