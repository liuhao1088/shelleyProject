}

.fight-master image {
  height: 100%;
  width: 100%;
  border-radius: 50%;
}

.user-item {
  width: 60rpx;
  height: 28rpx;
  line-height: 28rpx;
  border-radius: 14rpx;
  text-align: center;
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
  color: rgb(255, 255, 255);
  font-size: 20rpx;
  position: absolute;
  bottom: -5px;
  left: 28rpx;
}

.modal-box {
  height: 120rpx;
  width: 120rpx;
  line-height: 120rpx;
  opacity: 0.15;
  border-radius: 50%;
  border: 2rpx dashed black;
}

.join-but {
  height: 60rpx;
  width: 100%;
  margin-top: 40rpx;
}

.join-but button {
  line-height: 60rpx;
  border-radius: 10rpx;
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
  color: rgb(255, 255, 255);
  font-size: 28rpx;
}

/* 去拼团成功 */
.modal-title {
  width: 100%;
  line-height: 64rpx;
  font-size: 44rpx;
}

.modal-msg {
  line-height: 40rpx;
  opacity: 0.5;
  color: rgba(16, 16, 16, 100);
  font-size: 26rpx;
  padding: 20rpx 60rpx;
}

/* 问答 */
.question-wrap {
  width: 100%;
  height: 880rpx;
  border: 0rpx solid;
  background-color: unset;
}

.bg-img image {
  height: 100%;
  width: 100%;
  vertical-align: middle;
}

.modal-wrap {
  height: 65%;
  width: 65%;
  border: 0rpx solid black;
  position: absolute;
  top: 270rpx;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}

.question-item {
  width: 100%;
  border: 0rpx solid rgb(246, 43, 62);
}

.scroll-item {
  width: 100%;
  height: 100%;
  border: 0rpx solid black;
}

.question-butItem {
  width: 100%;
  height: 68rpx;
  border: 0rpx solid #bfa;
  display: flex;
  justify-content: center;
  margin-bottom: 36rpx;
}

.question-butItem button {
  width: 50%;
  height: 100%;
  background-image: linear-gradient(180deg, #df2e15, #f1705d, #df2e15);
  color: #fff;
  line-height: 68rpx;
  font-size: 32rpx;
  border-radius: 8rpx;
  transition: all 1s;
}

.title-item {
  width: 100%;
  line-height: 40rpx;
  color: rgba(0, 0, 0, 0.75);
  font-size: 28rpx;
  text-align: left;
  font-weight: bold;
  margin-bottom: 14rpx;
}

.car-lightsItem {
  width: 100%;
  border: 0rpx solid black;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-bottom: 10rpx;
}

.car-lightsBut {
  width: 45%;
  height: 56rpx;
}

.car-lightsBut button {
  height: 100%;
  width: 100%;
  line-height: 56rpx;
  border-radius: 8rpx;
  font-size: 28rpx;
}

.carLightsRed {
  background-color: #B91F09;
  color: rgb(255, 255, 255);
}

.red {
  background-color: rgba(0, 0, 0, 0.2);
}

.close-item {
  width: 100%;
  border: 0rpx solid black;
  color: rgb(221, 186, 51);
  font-size: 24rpx;
}

.tag-item {
  width: 60%;
  height: 28rpx;
  border: 0rpx solid red;
  position: relative;
  display: flex;
  justify-content: center;
}

.tag {
  height: 100%;
  line-height: 28rpx;
  color: rgb(200, 200, 200);
  font-size: 20rpx;
}

.tag::after {
  content: "";
  display: block;
  position: absolute;
  height: 0px;
  width: calc(60% - 120rpx);
  border-bottom: 1px solid #ccc;
  left: 0;
  top: 50%;
  z-index: 0;
}

.tag::before {
  content: "";
  display: block;
  position: absolute;
  height: 0px;
  width: calc(60% - 120rpx);
  border-bottom: 1px solid #ccc;
  right: 0;
  top: 50%;
  z-index: 0;
}