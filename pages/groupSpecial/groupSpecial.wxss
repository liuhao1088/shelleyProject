}

.rule-text {
  height: 40rpx;
  line-height: 40rpx;
  color: rgb(255, 255, 255);
  font-size: 30rpx;
}

/* 发起拼团弹窗 */
.cu-dialog {
  width: 580rpx;
}

.padding-xl {
  padding: 0rpx 60rpx;
  padding-bottom: 40rpx;
}

.cu-dialog {
  background-color: rgba(255, 255, 255, 100);
}

.icon-item {
  width: 100%;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  color: rgba(229, 7, 34, 0.5);
  font-weight: bold;
}

.modal-item {
  width: 100%;
  line-height: 50rpx;
  font-size: 28rpx;
  padding: 0 20rpx;
}

.modal-but {
  width: 100%;
  height: 80rpx;
  border: 0rpx solid red;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-around;
}

.but-item {
  width: calc(90%/2);
  height: 100%;
  border: 0rpx solid black;
  border-radius: 8rpx;
}

.but-item button {
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 8rpx;
}

.turn-but {
  background-color: rgb(255, 255, 255);
  color: rgb(229, 7, 34);
  border: 2rpx solid rgb(229, 7, 34);
}

.share-but {
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
  background-color: rgba(229, 7, 34);
  color: rgba(255, 255, 255);
}

/* 去拼团 */
.modal-userItem {
  width: 100%;
  display: flex;
  justify-content: space-evenly;
  margin-top: 20rpx;
}

.fight-master {
  height: 120rpx;
  width: 120rpx;
  line-height: 120rpx;
  color: rgb(255, 255, 255);
  border-radius: 50%;
  background-color: red;
