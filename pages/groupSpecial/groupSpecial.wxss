/* 发起拼团弹窗 */
.cu-dialog {
  width: 580rpx;
}

.padding-xl {
  padding: 0rpx 60rpx;
  padding-bottom: 40rpx;
}

.cu-dialog {
  background-color: rgba(255, 255, 255, 100);
}

.icon-item {
  width: 100%;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  color: rgba(105, 113, 254, 50);
}

.modal-item {
  width: 100%;
  line-height: 50rpx;
  font-size: 28rpx;
  padding: 0 20rpx;
}

.modal-but {
  width: 100%;
  height: 80rpx;
  border: 0rpx solid red;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-around;
}

.but-item {
  width: calc(90%/2);
  height: 100%;
  border: 0rpx solid black;
  border-radius: 8rpx;
}

.but-item button {
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 8rpx;
}

.turn-but {
  background-color: rgb(255, 255, 255);
  color: rgb(229, 7, 34);
  border: 2rpx solid rga(229, 7, 34);
}

.share-but {
  background-image: linear-gradient(133deg,#f92643, #e1001f ,#ee0049);
  background-color: rgba(229, 7, 34);
  color: rgba(255, 255, 255);
}

/* 去拼团 */
.modal-userItem {
  width: 100%;
  display: flex;
  justify-content: space-evenly;
  margin-top: 20rpx;
}

.fight-master {
  height: 120rpx;
  width: 120rpx;
  line-height: 120rpx;
  color: rgb(255, 255, 255);
  border-radius: 50%;
  background-color: red;
  position: relative;
}

.fight-master image {
  height: 100%;
  width: 100%;
  border-radius: 50%;
}

.user-item {
  width: 60rpx;
  height: 28rpx;
  line-height: 28rpx;
  border-radius: 14rpx;
  text-align: center;
  background-image: linear-gradient(133deg,#f92643, #e1001f ,#ee0049);
  color: rgb(255, 255, 255);
  font-size: 20rpx;
  position: absolute;
  bottom: -5px;
  left: 28rpx;
}

.modal-box {
  height: 120rpx;
  width: 120rpx;
  line-height: 120rpx;
  opacity: 0.15;
  border-radius: 50%;
  border: 2rpx dashed black;
}

.join-but {
  height: 60rpx;
  width: 100%;
  margin-top: 40rpx;
}

.join-but button {
  line-height: 60rpx;
  border-radius: 10rpx;
  background-image: linear-gradient(133deg,#f92643, #e1001f ,#ee0049);
  color: rgb(255, 255, 255);
  font-size: 28rpx;
}

/* 去拼团成功 */
.modal-title {
  width: 100%;
  line-height: 64rpx;
  font-size: 44rpx;
}

.modal-msg {
  line-height: 40rpx;
  opacity: 0.5;
  color: rgba(16, 16, 16, 100);
  font-size: 26rpx;
  padding: 20rpx 60rpx;
}

/* 问答 */
.modal-img {
  width: 100%;
  font-weight: 700;
}

.title-item {
  width: 100%;
  line-height: 48rpx;
  font-size: 30rpx;
}

.tag-item {
  width: 100%;
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx;
}

.modal-img image {
  height: 100%;
  width: 100%;
  vertical-align: middle;
}

.modal-wrap {
  width: 100%;
  border: 0rpx solid red;
  padding-top: 20rpx;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}

.modal-selectBut {
  width: 42%;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 20rpx;
}

.modal-selectBut:not(:nth-child(2n+1)) {
  margin-left: 20rpx;
}

.modal-selectBut button {
  height: 100%;
  padding: 0 20rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: #e5e5e5;
  font-size: 24rpx;
  color: rgba(32, 41, 55, 100);
  border-radius: 30rpx;
  font-weight: 600;
}

.modal-questionBut {
  width: 100%;
  height: 80rpx;
  margin-top: 40rpx;
}

.modal-questionBut button {
  height: 100%;
  width: 100%;
  line-height: 80rpx;
  border-radius: 8rpx;
  background-image: linear-gradient(133deg,#f92643, #e1001f ,#ee0049);
  color: rgb(255, 255, 255);
  font-size: 28rpx;
}