.coupon-price .group-price {
  color: rgb(255, 31, 19);
  font-size: 36rpx;
  font-weight: bold;
}

.coupon-butItem{
  flex: 1;
  border: 0rpx solid black;
  display: flex;
  justify-content: flex-end;
}

.group-butItem{
  height: 100%;
  width: 100rpx;
}

.group-butItem button{
  height: 100%;
  width: 100%;
  line-height: 50rpx;
  text-align: center;
  border-radius: 8rpx;
  font-size: 18rpx;
  padding: 0;
  font-weight: bold;
  background-color: #fff;
}

.but-initiate{
  background-image: linear-gradient(#ff8226,#ffb126);
  color: #fff;
}

.but-group{
  border: 2rpx solid rgb(250, 181, 54);
  color: rgb(250, 181, 54);
}

.but-group::after {
  border: none;
} 



