  display: flex;
  justify-content: space-between;
}

.coupon-title {
  height: 100%;
  font-size: 36rpx;
  color: rgb(16, 16, 16);
}

.coupon-address {
  height: 75%;
  line-height: 40rpx;
  color: rgb(128, 118, 118);
  font-size: 20rpx;
