  border-radius: 0px 0px 6px 6px;
  background-color: rgba(228, 228, 228, 0.35);
}

.coupon-swiper {
  height: 40rpx;
  width: 100%;
  border: 0rpx solid red;
}

.coupon-main {
  flex: 1;
  border: 0rpx solid black;
  color: rgb(128, 118, 118);
}

.coupon-wrap {
  width: 100%;
  height: 100%;
  line-height: 40rpx;
  font-size: 20rpx;
  border: 0rpx solid black;
  display: flex;
}

.coupon-user {
  display: flex;
  border: 0rpx solid orange;
}

.coupon-msg {
