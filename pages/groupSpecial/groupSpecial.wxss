.bgColor {
  background-color: #03080e;
}

image {
  height: 100%;
  width: 100%;
  vertical-align: middle;
}

.groupSpecialContainer {
  /* display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between; */
}

.wrap {
  height: 826rpx;
  width: 100%;
  border: 0rpx solid red;
}

.header-imgItem {
  width: 100%;
}

.group-countdownItem {
  height: 152rpx;
  width: 100%;
  border-radius: 56rpx 56rpx 0px 0px;
  background-color: rgb(250, 181, 54);
  padding: 24rpx 44rpx;
  display: flex;
  position: relative;
  top: -190rpx;
}

.group-titleItem {
  height: 100%;
  line-height: 50rpx;
  color: rgb(255, 255, 255);
  font-size: 50rpx;
  font-weight: bold;
}

.countdown-item {
  flex: 1;
  border: 0rpx solid red;
}

.wrap-title {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: right;
  font-size: 20rpx;
  color: #fff;
}

.tui-conutdown-box {
  display: inline-block;
  height: 30rpx;
  width: 30rpx;
  line-height: 30rpx;
  text-align: center;
  background-color: red;
  color: #fff;
  margin: 0 4rpx;
}

.tui-countdown-bg {
  background-color: #DF0101;
}

.group-mainItem {
  width: 100%;
  border-radius: 56rpx 56rpx 0px 0px;
  background-color: #fff;
  padding-top: 90rpx;
  position: relative;
  top: -240rpx;
}

.group-main {
  width: 100%;
  color: rgb(194, 194, 194);
  position: relative;
  top: -60rpx;
  padding-bottom: 40rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

.coupon-item {
  width: 90%;
  height: 200rpx;
  padding: 30rpx 20rpx;
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.36);
  display: flex;
}

.coupon-imgItem {
  height: 130rpx;
  width: 130rpx;
  border-radius: 10rpx;
}

.coupon-mainItem {
  flex: 1;
  border: 0rpx solid black;
  padding-left: 20rpx;
}

.coupon-titleItem {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  display: flex;
  justify-content: space-between;
}

.coupon-title {
  height: 100%;
  font-size: 36rpx;
  color: rgb(16, 16, 16);
}

.coupon-address {
  height: 75%;
  line-height: 40rpx;
  color: rgb(128, 118, 118);
  font-size: 20rpx;
  padding: 0 10rpx;
  border-radius: 0px 0px 6px 6px;
  background-color: rgba(228, 228, 228, 0.35);
}

.coupon-swiper {
  height: 40rpx;
  width: 100%;
  border: 0rpx solid red;
}

.coupon-main {
  flex: 1;
  border: 0rpx solid black;
  color: rgb(128, 118, 118);
}

.coupon-wrap {
  width: 100%;
  height: 100%;
  line-height: 40rpx;
  font-size: 20rpx;
  border: 0rpx solid black;
  display: flex;
}

.coupon-user {
  display: flex;
  border: 0rpx solid orange;
}

.coupon-msg {
  height: 100%;
  line-height: 40rpx;
  padding-left: 20rpx;
  border: 0rpx solid red;
}

.top_swiper {
  width: 100%;
  height: 100%;
}

.swiper-img {
  height: 100%;
  width: 30rpx;
  background-color: white;
  float: left;
}

.swiper-img image {
  height: 30rpx;
  width: 100%;
  border-radius: 50%;
  vertical-align: middle;
}

.swiper-name {
  height: 100%;
  line-height: 40rpx;
  justify-content: left;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  padding-left: 10rpx;
}

.coupon-time {
  flex: 1;
  line-height: 40rpx;
  text-align: right;
}

.coupon-priceItem {
  width: 100%;
  height: 50rpx;
  border: 0rpx solid red;
  display: flex;
}

.coupon-price {
  height: 100%;
  line-height: 50rpx;
  white-space: pre-wrap;
  color: rgb(128, 118, 118);
  font-size: 20rpx;
}

.coupon-price .group-price {
  color: rgb(255, 31, 19);
  font-size: 36rpx;
  font-weight: bold;
}

.coupon-butItem {
  flex: 1;
  border: 0rpx solid black;
  display: flex;
  justify-content: flex-end;
}

.group-butItem {
  height: 100%;
  width: 100rpx;
}

.group-butItem button {
  height: 100%;
  width: 100%;
  line-height: 50rpx;
  text-align: center;
  border-radius: 8rpx;
  font-size: 18rpx;
  padding: 0;
  font-weight: bold;
  background-color: #fff;
}

.but-initiate {
  background-image: linear-gradient(#ff8226, #ffb126);
  color: #fff;
}

.but-group {
  border: 2rpx solid rgb(250, 181, 54);
  color: rgb(250, 181, 54);
}

button::after {
  border: none;
}

/* 发起拼团弹窗 */
.cu-dialog {
  width: 580rpx;
}

.padding-xl {
  padding: 0rpx 60rpx;
  padding-bottom: 40rpx;
}

.cu-dialog {
  background-color: rgba(255, 255, 255, 100);
}

.icon-item {
  width: 100%;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  color: rgba(229, 7, 34, 0.5);
  font-weight: bold;
}

.modal-item {
  width: 100%;
  line-height: 50rpx;
  font-size: 28rpx;
  padding: 0 20rpx;
}

.modal-but {
  width: 100%;
  height: 80rpx;
  border: 0rpx solid red;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-around;
}

.but-item {
  width: calc(90%/2);
  height: 100%;
  border: 0rpx solid black;
  border-radius: 8rpx;
}

.but-item button {
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 8rpx;
}

.turn-but {
  background-color: rgb(255, 255, 255);
  color: rgb(229, 7, 34);
  border: 2rpx solid rgb(229, 7, 34);
}

.share-but {
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
  background-color: rgba(229, 7, 34);
  color: rgba(255, 255, 255);
}

/* 去拼团 */
.modal-userItem {
  width: 100%;
  display: flex;
  justify-content: space-evenly;
  margin-top: 20rpx;
}

.fight-master {
  height: 120rpx;
  width: 120rpx;
  line-height: 120rpx;
  color: rgb(255, 255, 255);
  border-radius: 50%;
  background-color: red;
  position: relative;
}

.fight-master image {
  height: 100%;
  width: 100%;
  border-radius: 50%;
}

.user-item {
  width: 60rpx;
  height: 28rpx;
  line-height: 28rpx;
  border-radius: 14rpx;
  text-align: center;
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
  color: rgb(255, 255, 255);
  font-size: 20rpx;
  position: absolute;
  bottom: -5px;
  left: 28rpx;
}

.modal-box {
  height: 120rpx;
  width: 120rpx;
  line-height: 120rpx;
  opacity: 0.15;
  border-radius: 50%;
  border: 2rpx dashed black;
}

.join-but {
  height: 60rpx;
  width: 100%;
  margin-top: 40rpx;
}

.join-but button {
  line-height: 60rpx;
  border-radius: 10rpx;
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
  color: rgb(255, 255, 255);
  font-size: 28rpx;
}

/* 去拼团成功 */
.modal-title {
  width: 100%;
  line-height: 64rpx;
  font-size: 44rpx;
}

.modal-msg {
  line-height: 40rpx;
  opacity: 0.5;
  color: rgba(16, 16, 16, 100);
  font-size: 26rpx;
  padding: 20rpx 60rpx;
}

/* 问答 */
.modal-img {
  width: 100%;
  font-weight: 700;
}

.title-item {
  width: 100%;
  line-height: 54rpx;
  font-size: 44rpx;
}

.tag-item {
  width: 100%;
  line-height: 40rpx;
  text-align: center;
  font-size: 24rpx;
  margin-top: 10rpx;
}

.modal-img image {
  height: 100%;
  width: 100%;
  vertical-align: middle;
}

.modal-wrap {
  width: 100%;
  border: 0rpx solid red;
  padding-top: 20rpx;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}

.modal-selectBut {
  width: 42%;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 20rpx;
  color: rgb(32, 41, 55);
}

.red {
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
  color: rgb(255, 255, 255);
}

.modal-selectBut:not(:nth-child(2n+1)) {
  margin-left: 20rpx;
}

.modal-selectBut button {
  height: 100%;
  padding: 0 20rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: #e5e5e5;
  font-size: 24rpx;
 
  border-radius: 30rpx;
  font-weight: 600;
}

.modal-questionBut {
  width: 100%;
  height: 80rpx;
  margin-top: 40rpx;
}

.modal-questionBut button {
  height: 100%;
  width: 100%;
  line-height: 80rpx;
  border-radius: 8rpx;
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
  color: rgb(255, 255, 255);
  font-size: 28rpx;
}

.rule-item{
  width: 120rpx;
  height: 92rpx;
  background-color: rgb(250, 181, 54);
  border-radius: 0px 46rpx 46rpx 0px;
  position: absolute;
  left: 0;
  top: 20%;
}

.rule-wrap{
  height: 54rpx;
  width: 60rpx;
  border: 0rpx solid black;
  font-size: 24rpx;
  color: rgb(255, 255, 255);
  text-align: center;
}

.text{
  width: 100%;
  height: 27rpx;
  line-height: 27rpx;
}

