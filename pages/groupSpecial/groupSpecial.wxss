.groupSpecialContainer {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0 30rpx;
}

.title {
  /* height: 50rpx; */
  width: 100%;
  text-align: right;
  opacity: 0.5;
  color: rgba(255, 255, 255, 100);
  font-size: 28rpx;
  padding: 20rpx 40rpx;
}

.wrap {
  width: 100%;
  background-color: #fff;
  border-radius: 40rpx;
  box-shadow: 2rpx 2rpx 12rpx 0px rgba(0, 0, 0, 0.4);
  padding: 50rpx 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.wrap-title {
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 40rpx;
  text-align: center;
}

.tui-conutdown-box {
  display: inline-block;
  height: 50rpx;
  width: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background-color: #000;
  color: #fff;
  margin: 0 10rpx;
}

.tui-countdown-bg {
  background-color: #DF0101;
}

.coupon-item {
  width: 95%;
  height: 250rpx;
  border-radius: 20rpx;
  text-align: center;
  box-shadow: 4rpx 4rpx 8rpx 0px rgba(0, 0, 0, 0.4);
  border: 2rpx solid rgba(255, 255, 255, 100);
  margin-top: 40rpx;
  display: flex;
  padding: 20rpx;
}

.img-item {
  height: 100%;
  width: 180rpx;
  background-color: black;
}

.wrap-item {
  flex: 1;
  border: 0rpx solid black;
  padding-left: 10rpx;
}

.coupon-titleItem {
  width: 100%;
  height: 44rpx;
  line-height: 44rpx;
  border: 0rpx solid red;
}

.coupon-title {
  float: left;
  font-size: 32rpx;
}

.coupon-address {
  opacity: 0.5;
  color: rgba(16, 16, 16, 100);
  font-size: 20rpx;
  float: right;
}

.coupon-group {
  width: 100%;
  height: 66rpx;
  border: 0rpx solid red;
  display: flex;
  margin-top: 20rpx;
}

.coupon-main {
  flex: 1;
  border: 0rpx solid black;
}

.coupon-but {
  height: 90%;
  width: 120rpx;
  border: 0rpx solid rebeccapurple;
}

.but {
  height: 100%;
  width: 100%;
  line-height: 64rpx;
  background-color: #6971FE;
  border-radius: 10rpx;
  background-color: rgba(105, 113, 254, 100);
  color: rgba(255, 255, 255, 100);
  font-size: 20rpx;
}

.group-price {
  height: 100%;
  line-height: 78rpx;
  color: rgba(255, 0, 0, 100);
  font-size: 40rpx;
  float: left;
}

.price-item {
  height: 100%;
  line-height: 88rpx;
  opacity: 0.5;
  color: rgba(32, 41, 55, 100);
  font-size: 20rpx;
  float: left;
  padding-left: 20rpx;
  text-decoration: line-through;
}

.coupon-wrap {
  width: 100%;
  line-height: 28rpx;
  font-size: 20rpx;
  border: 0rpx solid black;
  padding-right: 10rpx;
  display: flex;
}

.coupon-user {
  flex: 1;
  border: 0rpx solid orange;
}

.coupon-msg {
  height: 100%;
  line-height: 28rpx;
  border: 0rpx solid red;
}

.top_swiper {
  width: 100%;
  height: 100%;
}

.swiper-img {
  height: 100%;
  width: 26rpx;
  background-color: orangered;
  float: left;
}

.swiper-img image {
  height: 100%;
  width: 100%;
  border-radius: 50%;
  vertical-align: middle;
}

.swiper-name {
  height: 100%;
  line-height: 28rpx;
  float: left;
  display: block;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  padding-left: 10rpx;
}

/* 发起拼团弹窗 */
.cu-dialog {
  width: 580rpx;
}

.padding-xl {
  padding: 0rpx 60rpx;
  padding-bottom: 40rpx;
}

.cu-dialog {
  background-color: rgba(255, 255, 255, 100);
}

.icon-item {
  width: 100%;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  color: rgba(105, 113, 254, 50);
}

.modal-item {
  width: 100%;
  line-height: 50rpx;
  font-size: 28rpx;
  padding: 0 20rpx;
}

.modal-but {
  width: 100%;
  height: 80rpx;
  border: 0rpx solid red;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-around;
}

.but-item {
  width: calc(90%/2);
  height: 100%;
  border: 0rpx solid black;
  border-radius: 8rpx;
}

.but-item button {
  width: 100%;
  height: 100%;
  line-height: 80rpx;
  text-align: center;
  font-size: 28rpx;
  border-radius: 8rpx;
}

.turn-but {
  background-color: rgba(255, 255, 255, 100);
  color: rgba(91, 98, 217, 100);
  border: 2rpx solid rgba(91, 98, 217, 100);
}

.share-but {
  background-color: rgba(91, 98, 217, 100);
  color: rgba(255, 255, 255, 100);
}

/* 去拼团 */
.modal-userItem {
  width: 100%;
  display: flex;
  justify-content: space-evenly;
  margin-top: 20rpx;
}

.fight-master {
  height: 120rpx;
  width: 120rpx;
  line-height: 120rpx;
  color: rgba(255, 255, 255, 100);
  border-radius: 50%;
  background-color: red;
  position: relative;
}

.fight-master image {
  height: 100%;
  width: 100%;
  border-radius: 50%;
}

.user-item {
  width: 60rpx;
  height: 28rpx;
  line-height: 28rpx;
  border-radius: 14rpx;
  text-align: center;
  background-color: rgba(105, 113, 254, 100);
  color: rgba(255, 255, 255, 100);
  font-size: 20rpx;
  position: absolute;
  bottom: -5px;
  left: 28rpx;
}

.modal-box {
  height: 120rpx;
  width: 120rpx;
  line-height: 120rpx;
  opacity: 0.15;
  border-radius: 50%;
  border: 2rpx dashed black;
}

.join-but {
  height: 60rpx;
  width: 100%;
  margin-top: 40rpx;
}

.join-but button {
  line-height: 60rpx;
  border-radius: 10rpx;
  background-color: rgba(91, 98, 217, 100);
  color: rgba(255, 255, 255, 100);
  font-size: 28rpx;
}

/* 去拼团成功 */
.modal-title {
  width: 100%;
  line-height: 64rpx;
  font-size: 44rpx;
}

.modal-msg {
  line-height: 40rpx;
  opacity: 0.5;
  color: rgba(16, 16, 16, 100);
  font-size: 26rpx;
  padding: 20rpx 60rpx;
  /* padding: 20rpx 100rpx; */
}

/* 问答 */
.modal-img {
  height: 160rpx;
  width: 100%;
  background-color: black;
}

.modal-img image {
  height: 100%;
  width: 100%;
  vertical-align: middle;
}

.modal-wrap {
  width: 100%;
  border: 0rpx solid red;
  padding-top: 20rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.modal-selectBut {
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 20rpx;
}

.modal-selectBut:not(:nth-child(2n+1)) {
  margin-left: 20rpx;
}

.modal-selectBut button {
  height: 100%;
  padding: 0 20rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: #e5e5e5;
  font-size: 24rpx;
  color: rgba(32, 41, 55, 100);
  border-radius: 30rpx;
  font-weight: 600;
}

.line {
  border: 2rpx solid black;
}

.modal-questionBut {
  width: 100%;
  height: 80rpx;
  margin-top: 40rpx;
}

.modal-questionBut button {
  height: 100%;
  width: 100%;
  line-height: 80rpx;
  border-radius: 8rpx;
  background-color: rgba(91, 98, 217, 100);
  color: rgba(255, 255, 255, 100);
  font-size: 28rpx;
}