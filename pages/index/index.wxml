<view class="cu-modal {{modalName==0?'show':''}}" catchtouchmove="ture" style="background: rgba(0, 0, 0, 0.3);">
  <view class="cu-dialog">
    <image 
      src="https://img12.360buyimg.com/ddimg/jfs/t1/138733/28/20361/156371/5fe710f7Edabbaeb7/a0be7809e52ee7f9.png">
    </image>
    <view class="modal-wrap">
      <view class="icon-item">
          <text class="iconfont iconquxiao" bindtap="hideModal"></text>
      </view>
      <view class="main-item">
        <view class="main-titleItem">
          <view class="main-title">您是否有安装以下配件?</view>
          <view class="main-tag">Install the following auto parts</view>
        </view>

        <view class="main-selectItem">
          <view class="modal-selectBut" wx:for="{{checkbox}}" wx:key="id">
            <button style="{{item.checked?'border: none;background-color: rgba(10, 75, 177, 1);':''}}" bindtap="ChooseCheckbox" id="{{item.id}}">{{item.name}}</button>
          </view>
        </view>

        <view class="main-butItem">
          <view class="but" bindtap="getCoupon">
            <button>立即领取</button>
          </view>
        </view>

      </view>
      
    </view>
  </view>
</view>