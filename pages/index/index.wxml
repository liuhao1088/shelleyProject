<import src="/template/myTemplate.wxml"></import>
<authorize id="authorize" />
<view class="container">
  <view class="header">
    <view class="header-img">
      <image mode="widthFix"
        src="https://img10.360buyimg.com/ddimg/jfs/t1/132770/34/16653/157003/5fb7740cE97a601e0/9fafaa1a8d4edc4d.jpg">
      </image>
    </view>
    <view class="wrap">
      <view class="wrap-topbar">
        <view class="topbar">

          <view class="topbar-item">
            <view class="topbar-icon">
              <text class="iconfont iconqian icon"></text>
            </view>
            <view class="topbar-guild">
              <view class="topbar-title">拼团特惠</view>
              <view class="topbar-msg">超值拼团价，仅限今日</view>
            </view>
          </view>

          <!-- 用户 -->
          <view class="topbar-item" bindtap="toReserveStore">
            <view class="topbar-icon">
              <text class="iconfont iconstore icon" style=" font-size: 56rpx;"></text>
            </view>
            <view class="topbar-guild">
              <view class="topbar-title">预约门店</view>
              <view class="topbar-msg">立即预约零等待体验</view>
            </view>
          </view>

          <!-- 门店 -->
          <!-- <view class="topbar-item"  bindtap="toMyStore">
            <view class="topbar-icon">
              <text class="iconfont iconstore icon" style=" font-size: 56rpx;"></text>
            </view>
            <view class="topbar-guild">
              <view class="topbar-title">我的门店</view>
              <view class="topbar-msg">活动配置/门店管理</view>
            </view>
          </view> -->

          <!-- 品牌 -->
          <!-- <view class="topbar-item" bindtap="toActivityDetails">
            <view class="topbar-icon">
              <text class="iconfont iconstore icon" style=" font-size: 56rpx;"></text>
            </view>
            <view class="topbar-guild">
              <view class="topbar-title">品牌活动</view>
              <view class="topbar-msg">品牌促销活动配置</view>
            </view>
          </view> -->

        </view>

        <!-- 用户 -->
        <view class="wrap-coupons">
          <view class="wrap-couponsItem" bindtap="toMyCoupon">
            <text class="iconfont iconquan" style="margin-right: 15rpx;font-size:28rpx"></text>
            我的卡券</view>
          <view class="wrap-couponsItem" style="opacity: 0.5;font-size: 24rpx;text-align: center;">当前暂无可用券</view>
        </view>

        <!-- 门店 -->
        <!-- <view class="wrap-coupons">
          <view class="wrap-couponsItem" bindtap="toCarAppointment">
            <text class="iconfont iconyuyue" style="margin-right: 15rpx;font-size:28rpx"></text>
            车主预约</view>
          <view class="wrap-couponsItem" style="opacity: 0.5;font-size: 24rpx;text-align: center;">当前无预约待处理</view>
        </view> -->

        <!-- 品牌 -->
        <!-- <view class="wrap-coupons">
          <view class="wrap-couponsItem" bindtap="toStoreInformation">
            <text class="iconfont iconstore-fill" style="margin-right: 15rpx;font-size:28rpx"></text>
            门店管理</view>
          <view class="wrap-couponsItem" style="opacity: 0.5;font-size: 24rpx;text-align: center;" bindtap="toStoreInformation">点击查看门店</view>
        </view> -->

      </view>
      <view class="wrap-img"></view>
    </view>
    <!-- <image style="width: 90%;" mode="widthFix"
      src="https://img10.360buyimg.com/ddimg/jfs/t1/141864/20/15138/50740/5fb7777bEe18cf63d/5ff3fe7cbc5f0b87.png">
    </image> -->
  </view>

  <view class="header" style="width: 90%;margin-top: 50rpx;padding-bottom: 80rpx;">
    <view class="navItem" wx:for="{{url}}" wx:key="id" id="{{index}}" bindtap="toSubnav">
      <image mode="widthFix" src="{{item}}">
      </image>
    </view>
  </view>



  <view class="img-item">
    <image bindtap="toVideo" mode="widthFix"
      src="https://img10.360buyimg.com/ddimg/jfs/t1/126165/29/19314/16926/5fb78030E8d012bf0/959f408d5cc14b9b.png">
    </image>
  </view>


  <image mode="widthFix"
    src="https://img14.360buyimg.com/ddimg/jfs/t1/129869/16/16886/195207/5fbb2a0fE36f97702/5e59e714a36e1980.jpg">
  </image>

</view>

