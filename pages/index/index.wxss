@import "/template/myTemplate.wxss";
.container{
  background-color: #101721;
}

.header{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}

.header-img{
  width: 100%;
  height: 880rpx;
  border:0rpx solid red;
}

.wrap{
  width: 90%;
  height: 336rpx;
  margin:auto;
  border:2rpx solid red;
  border-radius: 60rpx;
  background-color: #fff;
}
.wrap-topbar{
  width: 100%;
  height: 216rpx;
  border:0rpx solid orange;
}

.wrap-img{
  width: 100%;
  height: 120rpx;
  border:2rpx solid #000;
}

.topbar{
  height: 80rpx;
  width: 100%;
  border:2rpx solid blue;
  margin-top: 30rpx;
}

.navItem{
  width: 47%;
  display: inline-block;
  margin-top: 60rpx;
}




