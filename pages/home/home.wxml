<authorize id="authorize" />
<view class="container" bindlongpress="toManage" style="padding-bottom: 120rpx;background-color: #101721;">
  <view class="swiper-wrapper">
    <view class="header">
      <view class="header-img">
        <image
          src="https://img14.360buyimg.com/ddimg/jfs/t1/148677/8/19473/316293/5fe2e232Ebe88b5dd/94894283bedee809.png">
        </image>
      </view>
      <image mode="widthFix" style="width:90%;margin: auto;"
        src="https://img11.360buyimg.com/ddimg/jfs/t1/160234/10/1810/111986/5ff52360E09cc8a0e/bce92d404a39eb96.png">
      </image>
    </view>
    <view class="wrap-productItem">
      <view class="product-imgItem">
        <view class="product-img" wx:for="{{url}}" wx:key="id" id="{{index}}" bindtap="toSubnav">
          <image src="{{item}}" mode="widthFix"></image>
        </view>
      </view>
    </view>

    <view class="img-item">
      <image bindtap="toVideo" mode="widthFix"
        src="https://img10.360buyimg.com/ddimg/jfs/t1/126165/29/19314/16926/5fb78030E8d012bf0/959f408d5cc14b9b.png">
      </image>
    </view>

  </view>

</view>

