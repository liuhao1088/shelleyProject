.myCouponContainer {
  height: 100%;
  background-color: rgba(27, 34, 46, 100);
  display: flex;
}

.wrapper {
  width: 90%;
  border-radius: 40rpx;
  background-color: #fff;
  margin: auto;
  margin-top: 60rpx;
  padding-bottom: 60rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.wrapper-item {
  width: 90%;
  height: 286rpx;
  margin-top: 40rpx;
  border-radius: 20rpx;
  box-shadow: 0 4rpx 10rpx 0 rgba(0, 0, 0, 0.4);
  background-color: #A9AEFE;
}

.wrap-title {
  width: 100%;
  height: 188rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
}

.wrap-mag {
  width: 100%;
  height: 98rpx;
  display: flex;
  justify-content: flex-end;
  background-color: #fff;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}

.wrap-titleItem {
  height: 66rpx;
  padding-top: 18rpx;
  margin-left: 20rpx;
  line-height: 66rpx;
  font-size: 32rpx;
  color: rgba(16, 16, 16, 100);
}

.wrap-summary {
  height: 78rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  color: rgba(16, 16, 16, 100);
  font-size: 24rpx;
}

.wrap-but {
  width: 190rpx;
  height: 100%;
  border: 0rpx solid red;
  display: flex;
  justify-content: center;
  align-items: center;
}

.but {
  height: 48rpx;
  width: 140rpx;
}

.but button {
  height: 100%;
  width: 100%;
  line-height: 48rpx;
  text-align: center;
  border-radius: 8rpx;
  background-color: rgba(105, 113, 254, 100);
  color: rgba(255, 255, 255, 100);
  font-size: 20rpx;
}

.wrap-tag {
  flex: 1;
  opacity: 0.66;
  color: rgba(16, 16, 16, 100);
  font-size: 24rpx;
  border: 0rpx solid bisque;
}

.wrap-dateItem {
  width: 100%;
  height: 28rpx;
  margin-top: 14rpx;
  margin-left: 20rpx;
  line-height: 28rpx;
}

.input-item {
  width: 50%;
  height: 60rpx;
  border: 0rpx solid red;
  margin: auto;
  margin-top: 40rpx;
  border-radius: 40rpx;
  border: 2rpx solid rgba(0, 0, 0, 1);
}

.input-item input{
  height: 100%;
  width: 100%;
  line-height: 100rpx;
  text-align: left;
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.but-item{
  height: 60rpx;
  width: 35%;
  margin: auto;
  margin-top: 50rpx;
  border: 0rpx solid red;
}

.but-item button{
  height: 100%;
  width: 100%;
  background-color: #5B62D9;
  border-radius: 10rpx;
  font-size: 28rpx;
  line-height: 60rpx;
  color: #fff;
}

.checkbox-item{
  width: 50%;
  height: 50rpx;
  margin: auto;
  margin-top: 20rpx;
  font-size: 22rpx;
  display: flex;
}

.checkbox{
  height: 100%;
  width: 50rpx;
}

.checkbox-msg{
  flex: 1;
  line-height: 48rpx;
}