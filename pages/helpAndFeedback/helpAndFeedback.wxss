.helpAndFeedbackContainer {
  padding-bottom: 160rpx;
}

.type-wrap {
  height: 260rpx;
  width: 90%;
  border: 0rpx solid red;
  margin-top: 40rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.type-item {
  height: 100rpx;
  width: 30%;
  border-radius: 20rpx;
  background-color: rgba(0, 0, 0, 0.05);
  color: rgba(16, 16, 16, 0.6);
  border: 0rpx solid black;
}

.type-mainItem {
  text-align: center;
  border: 0rpx solid red;
  font-size: 28rpx;
}

.icon-item {
  width: 100%;
  height: 50rpx;
  line-height: 60rpx;
}

.type-name {
  height: 40rpx;
  width: 100%;
  font-size: 24rpx;
}

.title-item {
  width: 90%;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 40rpx;
  font-size: 40rpx;
}

.line-red {
  border: 2rpx solid #f92643;
  color: #f92643;
  background-color: #fff;
}

.wrap-item {
  width: 90%;
  height: 460rpx;
  border-radius: 10rpx;
  background-color: rgb(240, 240, 240);
  margin-top: 10rpx;
}

.cu-form-group textarea {
  height: 200rpx;
  border: 0rpx solid black;
}

.img-item {
  height: 140rpx;
  width: 140rpx;
  margin-top: 20rpx;
  margin-left: 30rpx;
  border-radius: 20rpx;
  border: 2rpx solid rgba(187, 187, 187);
  border: 0rpx solid red;
}

.solids {
  height: 100%;
  width: 140rpx;
  position: relative;
}

.solids image {
  height: 100%;
  width: 100%;
  vertical-align: middle;
}

.grid.grid-square>view {
  margin-right: 0rpx;
  margin-bottom: 0rpx;
}

.grid.grid-square .cu-tag {
  background-color: transparent;
}

.bg-red {
  color: red;
  font-weight: bold;
}

.contact-details {
  margin-left: 20rpx;
  color: rgba(16, 16, 16, 0.5);
  font-size: 24rpx;
}

.input-item {
  width: 90%;
  height: 80rpx;
  border-radius: 10rpx;
  background-color: rgb(240, 240, 240);
  margin-top: 10rpx;
}

.input-item input {
  height: 100%;
  width: 100%;
  line-height: 80rpx;
  padding: 0 20rpx;
}

.but-item {
  width: 100%;
  height: 140rpx;
  border-top: 2rpx solid #e8e8e8;
  background-color: #fff;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-content: center;
  position: fixed;
  bottom: 0;
}

.but-item button {
  height: 80rpx;
  width: 90%;
  border-radius: 10rpx;
  color: rgb(255, 255, 255);
  font-size: 28rpx;
  text-align: center;
  background-image: linear-gradient(133deg, #f92643, #e1001f, #ee0049);
}