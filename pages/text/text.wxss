.wrap{
  width:200px;
  height: 160px;
  margin:200px auto;
  animation: drop 1s infinite;
}
.wrap view{
  width:200px;
  height: 160px;
  position: relative;
  animation: drop 4s infinite;
  background-color: transparent; /*test color*/
}
@keyframes drop{
  0%{
    transform:scale(1.2,1.2);
  }
  50%{
    transform:scale(1.5,1.5);
  }
  to{
    transform: scale(1.2,1.2);
  }
}
.wrap view:before{
  content: '';
  position: absolute;
  left:0;
  top:0;
  width:100px;
  height: 160px;
  border-radius:100px 100px 0 0;
  transform:rotate(45deg);
  transform-origin: right bottom;
  background-color: red;
}
.wrap view:after{
  content: '';
  position: absolute;
  right: 0;
  top:0;
  width: 100px;
  height: 160px;
  border-radius:100px 100px 0 0;
  background-color: red;
  transform:rotate(-45deg);
  transform-origin:left bottom;
}

/* 开关 */
.switch {
  position: relative;
  overflow: hidden;
  width: 140rpx;
  height: 60rpx;
  border-radius: 16px;
}

.switch::before {
  content: "";
  position: absolute;
  background-color: #D1D3D7;
  
}

.switch::after {
  position: absolute;
  display: inline-block;
  content: "";
  margin-top: 6rpx;
  height: 46rpx;
  width: 46rpx;
  border-radius: 50%;
  background-color: #fff;
 
 
}

.toggle-off {
  background-color: #e5e5e5;
}

.toggle-off::before {
  width: 140rpx;
  height: 60rpx;
}

.toggle-off::after {
  left: 3px;
  box-shadow: 1px 2px 4px #aaa;
}

.toggle-on::before {
  width: 0px;
  height: 0px;
  left: 40px;
  top: 15px;
}

.toggle-on:after {
  left: 46px;
}

.switch-enable{
  height: 100%;
  line-height: 60rpx;
  font-size: 28rpx;
  position: absolute;
  left: 20rpx;
  color: #fff;
  font-weight: bold;
}

.switch-disable{
  height: 100%;
  line-height: 60rpx;
  font-size: 28rpx;
  position: absolute;
  right: 20rpx;
  font-weight: bold;
  color: #fff;
}


